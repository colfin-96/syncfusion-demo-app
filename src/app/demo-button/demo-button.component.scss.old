/*#region ButtonSpecificValues*/
[ejs-button] {
  font-size: 14px;
  font-family: "Open Sans";
  font-weight: 600;

  &.fill {
    background-color: rgba(0, 130, 187, 1);

    min-width: 64px;
    color: #ffffff;
    height: 36px;
    box-shadow: 0px 1px 5px 0px rgba(0, 0, 0, 0.12),
      0px 2px 2px 0px rgba(0, 0, 0, 0.14), 0px 3px 1px -2px rgba(0, 0, 0, 0.2);
    border-radius: 4px;

    &:hover {
      background-color: rgba(0, 130, 187, 0.92);
    }

    &:focus {
      background-color: rgba(0, 130, 187, 0.76);
    }

    &:active {
      background-color: rgba(0, 130, 187, 0.76);
    }
  }

  &.outline {
    min-width: 64px;
    background-color: #ffffff;
    color: #0082bb;
    height: 36px;
    box-shadow: none;
    border: 1px #e0e0e0 solid;
    border-radius: 4px;
  }

  &.link {
    min-width: 64px;
    background-color: #ffffff;
    color: #0082bb;
    height: 36px;
    box-shadow: none;
  }
}

.float {
  background-color: #0082bb;
  color: #ffffff;
  height: 56px;
  width: 56px;
}
/*#endregion ButtonSpecificValues*/

/*#region ButtonGroupSingleSelect*/
.grouped {
  min-width: 64px;
  background-color: #ffffff;
  color: #616161;
  height: 36px;
  box-shadow: none;

  &:hover {
    // background-color: #0000000a;
    background-color: yellow;
  }
}

.e-btn-group:not(.e-outline),
.e-css.e-btn-group:not(.e-outline) {
  box-shadow: none;
}

.e-btn-group .e-btn:active,
.e-btn-group input:active + label.e-btn,
.e-btn-group input:checked + label.e-btn,
.e-css.e-btn-group .e-btn:active,
.e-css.e-btn-group input:active + label.e-btn,
.e-css.e-btn-group input:checked + label.e-btn {
  background-color: #0082bb0c;
  color: #0082bb;
  box-shadow: none;
}

*.e-btn-group:not(.e-rtl):not(.e-vertical) .e-btn:first-of-type,
*.e-css.e-btn-group:not(.e-rtl):not(.e-vertical) .e-btn:first-of-type {
  border-color: #e0e0e0;
  border-width: 1px;
  border-style: solid;
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
}

*.e-btn-group:not(.e-rtl):not(.e-vertical)
  .e-btn:not(:first-of-type):not(:last-of-type),
*.e-css.e-btn-group:not(.e-rtl):not(.e-vertical)
  .e-btn:not(:first-of-type):not(:last-of-type) {
  border-color: #e0e0e0;
  border-width: 1px;
  border-style: solid;
  border-left: none;
}

*.e-btn-group:not(.e-rtl):not(.e-vertical) .e-btn:last-of-type,
*.e-css.e-btn-group:not(.e-rtl):not(.e-vertical) .e-btn:last-of-type {
  border-color: #e0e0e0;
  border-width: 1px;
  border-style: solid;
  border-left: none;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
/*#endregion ButtonGroupSingleSelect*/

/*#region DemoValues*/
.container {
  display: block;
  grid-template-columns: repeat(auto-fit, minmax(1px, 1fr));
}

button {
  margin: 4px;
}

@font-face {
  font-family: "button-icons";
  src: url(data:application/x-font-ttf;charset=utf-8;base64,)
    format("truetype");
  font-weight: normal;
  font-style: normal;
}

.e-btn-sb-icons {
  font-family: "button-icons";
}

.e-add-icon::before {
  content: "\e70a";
}
/*#endregion DemoValues*/
